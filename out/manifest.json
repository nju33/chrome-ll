{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "3.0.0",
  "default_locale": "en",
  "omnibox": {
    "keyword": "ll"
  },
  "permissions": ["identity", "identity.email", "storage", "tabs"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "ll",
    "default_popup": "htmls/popup.html"
  },
  "background": {
    "persistent": false,
    "page": "htmls/background.html"
  },
  "content_security_policy": "script-src https://kit.fontawesome.com/1ba52c82c4.js 'self' 'unsafe-eval'; object-src 'self'"
}
